{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },

    "env": {
        "es6": true,
        "browser": true
    },

    "rules": {
        "no-nested-ternary": "off",
        "no-unused-vars": "warn"
    }
}
